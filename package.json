    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^9.4.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.4",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.54.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1",
    "url-loader": "^1.1.2",
    "vite": "^3.0.7",
    "vue-loader": "^15.7.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "^4.2.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
