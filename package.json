{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "vue template",
  "main": "app",
  "scripts": {
    "dist": "webpack --config webpack.prod.js",
    "build": "webpack --config webpack.dev.js",
    "watch": "vite serve",
    "devserver": "export NODE_ENV=development && node --inspect server/app",
    "server": "export NODE_ENV=production && nodemon server/app",
    "client": "export NODE_ENV=production && node client"
  },
  "keywords": [
    "vue",
    "vuex"
  ],
  "author": "Jeff Zhong",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-favicon": "^2.0.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^4.0.2",
    "mysql": "^2.18.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@vitejs/plugin-vue": "^3.0.3",
